{
  "displayName": "basalt-auth-tests",
  "preset": "ts-jest",
  "coverageReporters": ["json-summary"],
  "coverageDirectory": "./coverage",
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/Source/$1"
  },
  "testMatch": [
    "**/?(*.)+(spec).ts"
  ],
  "resetMocks": true,
  "clearMocks": true,
  "verbose": true
}
